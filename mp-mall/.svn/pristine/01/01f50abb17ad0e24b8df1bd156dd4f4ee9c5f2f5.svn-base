.seckill {
  overflow: hidden;
  min-height: 100vh;
  background-color: #F1F1F1;
}
.void_box {
  top: 50%;
  left: 50%;
  width: 200rpx;
  position: absolute;
  text-align: center;
  transform: translate(-50%, -60%);
}
.void_box image {
  width: 100rpx;
  height: 100rpx;
}
.void_box .void_txt {
  margin-top: 20rpx;
}
.seckill_tabs {
  /* overflow-x: auto;
  overflow-y: hidden; */
  padding-bottom: 20rpx;
}
.scroll-view {
  width: 100vw;
}
.seckill_tabs .tabs_box {
  display: flex;
  min-width: max-content;
  background-color: #fff;
}
.seckill_tabs .tabs_box .box_item {
  /* margin: 0 10rpx; */
  display: inline-block;
  padding: 6rpx 16rpx;
  position: relative;
  text-align: center;
  /* min-width: 204rpx;
  box-sizing: border-box; */
  /* background-color: #EBEBEB; */
}
.seckill_tabs .tabs_box .box_item .item_time {

}
.seckill_tabs .tabs_box .box_item .item_txt {
  color: #999;
  font-size: 20rpx;
  margin-top: 4rpx;
}
.seckill_tabs .tabs_box .box_item.on {
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.seckill_tabs .tabs_box .box_item.on .item_time,
.seckill_tabs .tabs_box .box_item.on .item_txt {
  color: #fff;
}
.seckill_tabs .tabs_box .box_item.active {
  /* background-color: #01c801; */
  /* background: linear-gradient(to right, #5ad563 0%, #36af62 100%); */
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.seckill_tabs .tabs_box .box_item.active .item_time,
.seckill_tabs .tabs_box .box_item.active .item_txt {
  color: #fff;
}
.seckill_tabs .tabs_box .box_item.active::after {
  right: 50rpx;
  bottom: -39rpx;
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  border-top: 20rpx solid #60b533 ;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid transparent;
  border-bottom: 20rpx solid transparent;
}


.seckill_box {
  margin: 0 20rpx 10rpx;
  padding: 0 16rpx;
  border-radius: 10rpx;
  background-color: #fff;
}

.goods_item {
  padding: 20rpx 0rpx 0;
  display: flex;
  border-bottom: 1rpx solid #f4f4f4;
}
.goods_item .goods_image {
  width: 220rpx;
  height: 220rpx;
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
}
.goods_item .goods_image .seckill_tip {
  top: 10rpx;
  right: 10rpx;
  width: 60rpx;
  position: absolute;
}
.goods_item .goods_image image {
  width: 100%;
  height: 100%;
}
.goods_item .goods_image .img_tip {
  top: 10rpx;
  right: 10rpx;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
}
 .img_tip.seckill_tip {
  width: 100rpx;
}
 .img_tip image {
  width: 100%;
  height: 100%;
}
.goods_info {
  flex: 1;
  margin-left: 20rpx;
  padding-bottom: 20rpx;
}
.goods_info > view {
  font-size: 26rpx;
  overflow-y: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.goods_desc {
  color: #AAAAAA;
}
.goods_info .goods_tit {
  font-size: 30rpx;
  -webkit-line-clamp: 2;
  position: relative;
}
.goods_info .goods_tit .tab {
  display: inline-block;
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  text-align: center;
  border-radius: 10rpx;
  overflow: hidden;
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.goods_info .goods_tit .tab.active {
  color: #fefb58;
  background: linear-gradient(to right, #f54327 0%, #f54327 100%);
}
.goods_info .goods_tit .tab image {
  width: 100%;
  height: 100%;
}
.goods_info .goods_tit .text {
  font-size: 30rpx;
  display: -webkit-box;
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}
.goods_info .goods_tit .text.tab_txt {
  text-indent: 150rpx;
}
.goods_btn {
  font-size: 26rpx;
  position: relative;
  border-radius: 40rpx;
  padding: 10rpx 20rpx;
  background-color: #d81e06;
}
.goods_sales .btn {
  width: 60rpx;
  height: 60rpx;
}
.goods_sales .btn image {
width: 100%;
height: 100%;
}
.goods_btn view {
  color: #fff;
}
.goods_btn .spot {
  top: 5px;
  right: 5px;
  width: 10rpx;
  height: 10rpx;
  position: absolute;
  border-radius: 50%;
  background-color: #d81e06;
}
.goods_num {
  padding: 0 20rpx;
}
.goods_info .goods_price {
  display: flex;
  padding: 10rpx 0;
  align-items: center;
  justify-content: space-between;
}
.goods_info .goods_price .price_box {
  display: flex;
  align-items: flex-end;
}
.price_box text {
 margin-right: 10rpx;
}
.price_box .price {
  color: #999;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.goods_price .price_now {
  display: flex;
  align-items: flex-end;
}
.goods_price .price_now .util {
  color: #FF0000;
  font-size: 20rpx;
}
.goods_price .price_now .num {
  color: #FF0000;
  font-size: 32rpx;
  height: 40rpx;
  font-weight: 700;
}
.goods_price .price_old {
  color: #E0E0E0;
  font-size: 26rpx;
  text-decoration: line-through;
}
.goods_info .goods_sales {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}
.goods_info .goods_sales .sales_num  {
  display: flex;
  margin-bottom: 10rpx;
}
.goods_info .goods_sales .sales_num .old {
  font-size: 20rpx;
  color: #333;
  text-decoration: line-through;
}
.goods_info .goods_sales .sales_num .now {
  font-size: 20rpx;
  color: #333;
  margin-left: 10rpx;
}
.goods_info .goods_sales .sales_num .sales_txt {
  font-size: 20rpx;
  color: #999;
  margin-left: 10rpx;
}
.goods_info .amount {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.goods_info .amount .amount_fl {
  width: 220rpx;
  display: flex;
  margin-right: 40rpx;
}
.goods_info .amount .amount_fl .progress {
  width: 100%;
  height: 30rpx;
  overflow: hidden;
  position: relative;
  border-radius: 30rpx;
  background-color: #ddd;
}
.goods_info .amount .amount_fl .progress .progress_box {
  height: 100%;
  border-radius: 30rpx;
  background-color: #60b533;
} 
.goods_info .amount .amount_fl .progress .progress_txt {
  top: 0;
  left: 0;
  color: #fff;
  position: absolute;
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.goods_info .amount .amount_item {
  padding: 10rpx 16rpx;
  border-radius: 10rpx;
  text-align: center;
  background-color: #eee;
}
.goods_info .amount .amount_item .amount_txt {
  font-size: 18rpx;
}
.goods_info .amount .amount_item .amount_tit {
  color: #999;
  font-size: 18rpx;
}
.goods_info .amount .amount_ls {
  display: flex;
  margin-left: 10rpx;
  border-radius: 10rpx;
  background-color: #eee;
}
.goods_info .amount .amount_ls .amount_item {
  border-right: 1rpx solid #ddd;
}
.goods_info .amount .amount_ls .amount_item:last-child {
  border-right: none;
}
.goods_info .amount .btn {
  /* width: 110rpx; */
  width: 80rpx;
  height: 80rpx;
  /* height: 40rpx; */
  color: #fff;
  text-align: center;
  border-radius: 40rpx;
  /* background: linear-gradient(to right, #60b533  0%, #60b732 100%); */
}
.goods_info .amount .btn.dis {
  /* background-color: #EBEBEB; */
  /* background: linear-gradient(to right, #ddd  0%, #ddd 100%); */
}
.goods_info .amount .btn image {
  width: 100%;
  height: 100%;
}