/* pages/user/user.wxss */
.user{
  overflow: hidden;
  min-height: 100vh;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  background-color: #f2f2f2;
}

.userinfo {
  margin: 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.user_box {
  padding: 20rpx;
  display: flex;
  border-radius: 10rpx;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
}
.user_msg {
  height: 154rpx;
  position: relative;
  margin-right: 10rpx;
}
.user_msg.act::after {
  top: 8rpx;
  right: 0rpx;
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-radius: 10rpx;
  position: absolute;
  background-color: #f54327;
}
.user_msg image {
  width: 38rpx;
  height: 48rpx;
}
.user_set {
  height: 166rpx;
}
.user_set image {
  width: 60rpx;
  height: 60rpx;
}
.arrow {
  width: 14rpx;
  height: 14rpx;
  transform: rotateZ(-45deg);
  border-right: 2rpx solid #ffffff;
  border-bottom: 2rpx solid #ffffff;
}
.userinfo .name {
  color: #333;
  padding: 20rpx 0;
}
.userinfo .name open-data {
  color: #333;
  font-size: 32rpx;
}
.user_box > view,.integral {
  padding: 10rpx 0;
}
.user_box .member {
  width: 90%;
  height: 150rpx;
  padding: 20rpx;
  border-radius: 20rpx 20rpx 0 0;
  box-sizing: border-box;
  /* background-image: linear-gradient(123deg, #453615 0%, #271C02 100%); */
}
.member_info {
  display: flex;
  padding: 20rpx 0;
  align-items: center;
  justify-content: space-between;
}

.member_info image {
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}
.member view,.member text {
 color: #FFC236;
}
.member_fl text {
  padding-left: 20rpx;
}
.integral {
  display: flex;
  align-items: center;
}
.integral.no_member {
  padding: 6rpx 20rpx;
  border-radius: 20rpx;
  background-color: #F1BE4A;
}
.integral.no_member text {
  font-size: 24rpx;
  color: #7F5800;
  padding-right: 10rpx;
}
.integral.no_member .arrow {
  border-right: 2rpx solid #7F5800;
  border-bottom: 2rpx solid #7F5800;
}
.integral text {
  padding-right: 20rpx;
}
.user_box .avatar {
  padding: 4rpx;
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  border-radius: 50%;
  background-color: #fff;
}
.user_box .user_info {
  flex: 1;
  padding-left: 20rpx;
}
.user_box .user_info .name {
  display: flex;
  align-items: center;
}
.user_box .user_info .name .user_name {
  color: #333;
  font-size: 32rpx;
}
.user_box .user_info .info_item {
  color: #333;
}
.user_box .user_info .auth {
  width: 120rpx;
  height: 36rpx;
  display: flex;
  margin-left: 20rpx;
  border-radius: 36rpx;
  line-height: 36rpx;
  align-items: center;
  justify-content: center;
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.user_box .user_info .auth .txt {
  color: #FFf;
  font-size: 22rpx;
}
.user_box .user_info .auth image {
  width: 30rpx;
  height: 30rpx;
  margin-right: -10rpx;
}
.avatar image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.avatar .img {
  width: 100%;
  height: 100%;
  overflow: hidden;
  border-radius: 50%;
}
.avatar open-data {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.module {
  padding: 0 20rpx;
}
.orider {
  padding: 0 20rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.orider .tit {
  color: #2E2E33;
  padding: 20rpx 0;
  font-size: 32rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #F0F0F0;
}
.tit_txt {
  color:  #1D202B;
}
.all_orider {
  display: flex;
  align-items: center;
}
.all_orider view {
  color: #9B9B9B;
}
.all_orider view:first-child {
  padding-right: 20rpx;
}
.orider_arrow {
  width: 12rpx;
  height: 12rpx;
  transform: rotateZ(-45deg);
  border-right: 2rpx solid #DCDCDC;
  border-bottom: 2rpx solid #DCDCDC;
}
.orider_list {
  display: flex;
  padding: 40rpx 0 0;
  justify-content: space-between;
}
.orider_item {
  width: 18%;
  text-align: center;
}
.item_ico {
  left: 50%;
  width: 60%;
  height: 60rpx;
  position: relative;
  transform: translateX(-50%);
}
.item_ico image {
  width: 100%;
  height: 100%;
}
.item_ico .num {
  top: -16rpx;
  right: -20rpx;
  color: #fff;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  border-radius: 50%;
  position: absolute;
  text-align: center;
  background-color: #FF5F58;
}
.item_txt {
  color: #828285;
  padding: 20rpx 0;
  font-size: 24rpx;
}
.menu_item {
  display: flex;
  padding: 24rpx;
  background-color: #fff;
  justify-content: space-between;
}
.tell {
  color: #007AFf;
}
.module_list {
  display: flex;
  padding: 20rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
  justify-content: space-around;
}
.module_item {
  width: 25%;
  padding: 20rpx 0;
  text-align: center;
}
button.module_item:not([size='mini']) {
  width: 25%;
  background-color: transparent;
}
.module_item image {
  width: 80rpx;
  height: 80rpx;
}
.module_item view {
  color: #9B9B9B;
  font-size: 24rpx;
  padding: 10rpx 0 0;
  /* padding-left: 20rpx; */
}
.quit {
  padding: 20rpx;
  color: #EF2322;
  margin-top: 20rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: #fff;
}
.advert_box {
  margin-top: 20rpx;
}
.advert_box .advert {
  width: 100%;
  height: 160rpx;
}
.advert_box .advert image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.weui-panel {
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.weui-media-box {
  display: flex;
  padding-top: 10rpx;
  align-items: flex-end;
  justify-content: space-between;
}
.weui-panel__hd text {
    color: #b2b2b2;
    font-size: 22rpx;
}
.weui-media-box__hd {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}
.weui-media-box__desc {
  color: #828285;
  font-size: 24rpx;
  overflow: hidden;
  padding-top: 10rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.weui-media {
  flex: 1;
  padding: 0px 20rpx;
}
.weui-media-box__title {
  color: #222;
  font-size: 32rpx;
}
.weui-media-box__hd image {
  width: 100%;
  height: 100%;
}
.weui-cell__ft {
  margin-bottom: 20rpx;
}
.weui-btn:not([size='mini']) {
  padding: 0;
  width: 120rpx;
  height: 50rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  font-weight: normal;
}

.recom {
  margin-top: 20rpx;
}
.recom .recom_tit {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.recom .recom_tit .recom_tit_bx {
  display: flex;
  align-items: center;
  justify-content: center;
}
.recom .recom_tit .recom_tit_bx .div {
  width: 30rpx;
  height: 4rpx;
  margin: 0 10rpx;
  background-color: #333;
}
.recom .recom_tit .recom_tit_bx .tit {
  font-weight: 700;
}
.recom .recom_box {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.recom .recom_box .recom_item {
  width: 49%;
  margin-right: 2%;
  overflow: hidden;
  margin-top: 10rpx;
  border-radius: 10rpx;
}
.recom .recom_box .recom_item:nth-child(2n) {
  margin-right: 0;
}
.recom .recom_box .recom_item .item_img {
  width: 100%;
  height: 318rpx;
  background-color: #fff;
}
.recom .recom_box .recom_item .item_img image {
  width: 100%;
  height: 100%;
}
.recom .recom_box .recom_item .item_info {
  padding: 20rpx;
  background-color: #fff;
}
.recom .recom_box .recom_item .item_info .info_tit {
  display: flex;
  margin: 10rpx 0;
  align-items: center;
}
.recom .recom_box .recom_item .item_info .info_tit .tab {
  display: inline-block;
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  overflow: hidden;
  /* position: absolute; */
  text-align: center;
  border-radius: 10rpx;
  /* background-color: #c69c6d; */
  /* background: linear-gradient(to right, #5ad563 0%, #36af62 100%); */
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.recom .recom_box .recom_item .item_info .info_tit .text {
  flex: 1;
  font-size: 30rpx;
  overflow: hidden;
  margin-left: 4rpx;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.recom .recom_box .recom_item .item_info .info_tit .text.tab_txt {
  text-indent: 150rpx;
}
.recom .recom_box .recom_item .item_info .add_car {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  justify-content: space-between;
}
.recom .recom_box .recom_item .item_info .in_fl {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price .util {
  font-size: 20rpx;
  color: #FF0000;
}
.recom .recom_box .recom_item .item_info .in_fl .price .num {
  font-size: 34rpx;
  height: 40rpx;
  color: #FF0000;
  font-weight: 700;

}
.recom .recom_box .recom_item .item_info .in_fl .price .price_txt {
  font-size: 20rpx;
}
.recom .recom_box .recom_item .item_info .in_fl .sales {
  color: #999;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr {
  width: 40rpx;
  height: 40rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr image {
  width: 100%;
  height: 100%;
}
