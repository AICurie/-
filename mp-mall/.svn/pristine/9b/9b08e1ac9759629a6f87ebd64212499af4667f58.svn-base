/* pages/orider/orider.wxss */
.tabber {
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  display: flex;
  position: fixed;
  box-sizing: border-box;
  background-color: #fff;
  justify-content: space-around;
  border-bottom: 1rpx solid #F2F2F2;
}
.orider_box {
  height: 100vh;
  padding-top: 120rpx;
  box-sizing: border-box;
}
.tabber_item {
  /* width: 25%; */
  padding: 40rpx 10rpx;
  text-align: center;
  border-bottom: 1rpx solid transparent;
}
.tabber_item.active {
  /* color: #FF5F58; */
  border-bottom: 1rpx solid #60b533;
}
.orider_item {
  padding: 40rpx;
  border-bottom: 1rpx solid #F2F2F2;
}
.goods_tit {
  display: flex;
  justify-content: space-between;
}
.goods_status {
  min-width: 100rpx;
}
.goods_time {
  color: #BDBDBD;
  font-size: 26rpx;
}
.red {
  color: #EF2322;
}
.hui {
  color: #828282;
}
.goods_nav {
  display: flex;
  padding: 40rpx 0;
  align-items: center;
  justify-content: space-between;
}
.goods_img {
  flex: 1;
  display: flex;
}
.goods_img image {
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
}
.goods_img image:last-child {
 margin-right: 0;
}
.goods_all {
  display: flex;
  align-items: center;
  /* padding-left: 40rpx; */
}
.goods_num {
  padding-right: 20rpx;
}
.arrow {
  width: 20rpx;
  height: 20rpx;
  transform: rotateZ(-45deg);
  border-right: 1rpx solid #333333;
  border-bottom: 1rpx solid #333333;
}
.goods_price {
  display: flex;
  padding: 10rpx 0;
  justify-content: space-between;
  /* border-bottom: 1rpx solid #F2F2F2; */
}
.price_num {
  color: #EF2322;
}
.fn_list {
  display: flex;
  padding: 30rpx 0;
  justify-content: flex-end;
}
.fn_item {
  margin-left: 40rpx;
  padding: 10rpx 30rpx;
  border-radius: 30rpx;
  border: 1rpx solid #797979;
}
.fn_item.active {
  /* color: #EF2322; */
  color: #60b533;
  font-weight: 700;
  /* border: 1rpx solid #EF2322; */
  border: 1rpx solid #60b533;
}

.vido {
  width: 100%;
  text-align: center;
  padding-top: 200rpx;
}
.vido image {
  width: 50%;
}


.recom {
  /* padding-top: 20rpx; */
  padding: 20rpx 10rpx;
  background-color: #f2f2f2;
}
.recom .recom_tit {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
}
.recom .recom_tit .recom_tit_bx {
  display: flex;
  align-items: center;
  justify-content: center;
}
.recom .recom_tit .recom_tit_bx .div {
  width: 30rpx;
  height: 4rpx;
  margin: 0 10rpx;
  background-color: #333;
}
.recom .recom_tit .recom_tit_bx .tit {
  font-weight: 700;
}
.recom .recom_box {
  display: flex;
  flex-wrap: wrap;
  margin-top: 10rpx;
}
.recom .recom_box .recom_item {
  width: 49%;
  margin-top: 10rpx;
  margin-right: 2%;
  overflow: hidden;
  border-radius: 10rpx;
}
.recom .recom_box .recom_item:nth-child(2n) {
  margin-right: 0;
}
.recom .recom_box .recom_item .item_img {
  width: 100%;
  height: 318rpx;
}
.recom .recom_box .recom_item .item_img image {
  width: 100%;
  height: 100%;
}
.recom .recom_box .recom_item .item_info {
  padding: 20rpx;
  background-color: #fff;
}
.recom .recom_box .recom_item .item_info .info_tit {
  display: flex;
  margin: 10rpx 0;
  align-items: center;
}
.recom .recom_box .recom_item .item_info .info_tit .tab {
  display: inline-block;
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  text-align: center;
  border-radius: 10rpx;
  /* background-color: #c69c6d; */
  /* background:  linear-gradient(to right, #5ad563 0%, #36af62 100%); */
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.recom .recom_box .recom_item .item_info .info_tit .text {
  flex: 1;
  font-size: 30rpx;
  overflow: hidden;
  display: inline-block;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.recom .recom_box .recom_item .item_info .info_tit .text.tab_txt {
  text-indent: 150rpx;
}
.recom .recom_box .recom_item .item_info .add_car {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  justify-content: space-between;
}
.recom .recom_box .recom_item .item_info .in_fl {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price .util {
  font-size: 20rpx;
  color: #FF0000;
}
.recom .recom_box .recom_item .item_info .in_fl .price .num {
  font-size: 34rpx;
  height: 40rpx;
  color: #FF0000;
  font-weight: 700;

}
.recom .recom_box .recom_item .item_info .in_fl .price .price_txt {
  font-size: 20rpx;
}
.recom .recom_box .recom_item .item_info .in_fl .sales {
  color: #999;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr {
  width: 40rpx;
  height: 40rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr image {
  width: 100%;
  height: 100%;
}