
.bar_header {
  top: 0;
  width: 100%;
  transition: .3s;
  background-color: #fff;
}
.bar_header.active{
  background-color: #1AE11A;
}
.bar_item {
  left: 0;
  width: 100%;
  z-index: 1;
  transition: .3s;
  display: flex;
  position: fixed;
  align-items: flex-end;
  background-color: #fff;
}
.bar_item.active {
  background-color: #1AE11A;
}
.bar_item > text {
  padding: 0 20rpx;
  font-size: 46rpx;
}
.bar_item .search {
  display: flex;
  padding: 0rpx 10rpx;
  font-size: 30rpx;
  min-width: 360rpx;
  align-items: center;
  color: #999;
  background-color: #f5f5f5;
  border-radius: 32rpx;
}
.bar_item .search image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.search_box {
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  z-index: 1;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  background-color: #fff;
  position: fixed;
  box-sizing: border-box;
}
 .search {
   /* flex: 1; */
  display: flex;
  padding: 10rpx 20rpx;
  font-size: 32rpx;
  align-items: center;
  background-color: #F2F2F2;
  border-radius: 60rpx;
  justify-content: space-between;
}
.search_fl {
  flex: 1;
  display: flex;
}
.search_fl input {
  flex: 1;
}
.cancel {
  color: #EF2322;
  padding: 20rpx;
}
 .search_fl image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.search_fr {
  width: 50rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.search_fr image {
  width: 32rpx;
  height: 32rpx;
}
.history_tit {
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
}
.history_tit view {
  color: #1D202B;
  font-weight: 700;
}
.del {
  width: 40rpx;
  height: 40rpx;
}
.history_tit image {
  width: 32rpx;
  height: 32rpx;
}
.history_list {
  display: flex;
  flex-wrap: wrap;
}
.history_item {
  margin: 20rpx;
  color: #9B9B9B;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
  border-radius: 30rpx;
  background-color: #f5f5f5;
}
.goods_list {
  background-color: #fff;
}

.goods_item {
  padding: 20rpx 20rpx 0;
  display: flex;
  border-bottom: 1rpx solid #f4f4f4;
}
.goods_item .goods_image {
  width: 220rpx;
  height: 220rpx;
  position: relative;
}
.goods_item .goods_image image {
  width: 100%;
  height: 100%;
}
.goods_item .goods_image {
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
  position: relative;
}
.goods_item .goods_image image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.goods_item .goods_image .img_tip {
  top: 10rpx;
  right: 10rpx;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
}
.goods_info {
  flex: 1;
  margin-left: 20rpx;
  padding-bottom: 20rpx;
}
.goods_info > view {
  font-size: 26rpx;
  overflow-y: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.goods_desc {
  color: #AAAAAA;
}
.goods_info .goods_tit {
  font-size: 30rpx;
  -webkit-line-clamp: 2;
  position: relative;
}
.goods_info .goods_tit .tab {
  display: inline-block;
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  text-align: center;
  border-radius: 10rpx;
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.goods_info .goods_tit .tab image {
  width: 100%;
  height: 100%;
}
.goods_info .goods_tit .text {
  font-size: 30rpx;
  overflow: hidden;
  display: -webkit-box;
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}
.goods_info .goods_tit .text.tab_txt {
  text-indent: 150rpx;
}
.goods_btn {
  font-size: 26rpx;
  position: relative;
  border-radius: 40rpx;
  padding: 10rpx 20rpx;
  background-color: #d81e06;
}
.goods_sales .btn {
  width: 60rpx;
  height: 60rpx;
}
.goods_sales .btn image {
width: 100%;
height: 100%;
}
.goods_btn view {
  color: #fff;
}
.goods_btn .spot {
  top: 5px;
  right: 5px;
  width: 10rpx;
  height: 10rpx;
  position: absolute;
  border-radius: 50%;
  background-color: #d81e06;
}
.goods_num {
  padding: 0 20rpx;
}
.goods_info .goods_price {
  display: flex;
  padding: 10rpx 0;
  align-items: center;
  justify-content: space-between;
}
.goods_info .goods_price .price_box {
  display: flex;
  align-items: flex-end;
}
.price_box text {
 margin-right: 10rpx;
}
.price_box .price {
  color: #999;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.goods_price .price_now {
  display: flex;
  align-items: flex-end;
}
.goods_price .price_now .util {
  color: #FF0000;
  font-size: 20rpx;
}
.goods_price .price_now .num {
  color: #FF0000;
  font-size: 32rpx;
  height: 40rpx;
  font-weight: 700;
}
.goods_price .price_old {
  color: #E0E0E0;
  font-size: 26rpx;
  text-decoration: line-through;
}
.goods_info .goods_sales {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}
.goods_info .goods_sales .sales_num  {
  display: flex;
  margin-bottom: 10rpx;
}
.goods_info .goods_sales .sales_num .old {
  font-size: 20rpx;
  color: #333;
  text-decoration: line-through;
}
.goods_info .goods_sales .sales_num .now {
  font-size: 20rpx;
  color: #333;
  margin-left: 10rpx;
}
.goods_info .goods_sales .sales_num .sales_txt {
  font-size: 20rpx;
  color: #999;
  margin-left: 10rpx;
}
.goods_info .amount {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.goods_info .amount .amount_fl {
  display: flex;
}
.goods_info .amount .amount_item {
  padding: 10rpx 16rpx;
  border-radius: 10rpx;
  text-align: center;
  background-color: #eee;
}
.goods_info .amount .amount_item .amount_txt {
  font-size: 20rpx;
}
.goods_info .amount .amount_item .amount_tit {
  color: #999;
  font-size: 20rpx;
}
.goods_info .amount .amount_ls {
  display: flex;
  margin-left: 10rpx;
  border-radius: 10rpx;
  background-color: #eee;
}
.goods_info .amount .amount_ls .amount_item {
  border-right: 1rpx solid #ddd;
}
.goods_info .amount .amount_ls .amount_item:last-child {
  border-right: none;
}
.goods_info .amount .btn {
  width: 50rpx;
  height: 50rpx;
}
.goods_info .amount .btn image {
  width: 100%;
  height: 100%;
}
.vido {
  width: 100%;
  text-align: center;
  padding-top: 200rpx;
}
.vido image {
  width: 50%;
}