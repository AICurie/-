.start {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
}
.start image {
  width: 100%;
  height: 100%;
}
.start .skip {
  right: 40rpx;
  bottom: 40rpx;
  color: #fff;
  position: fixed;
  border-radius: 28rpx;
  padding: 10rpx 20rpx;
  background-color: #40444f;
}
