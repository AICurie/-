  color: #b6b6b6;
  background-color: #f7f7f7;
  border-radius: 26rpx;
}
.bar_item .search image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}

.header_top {
  padding: 0 20rpx 20rpx;
  /* display: flex; */
  /* justify-content: space-between; */
  /* align-items: center; */
  background-color: #fff;
}
.header_top.active {
  height: 260rpx;
  background-image: linear-gradient(179deg, #EF2322 0%, rgba(255,255,255,0.00) 100%);
}
.news_tips {
  z-index: 10;
  position: relative;
}
.news_tips .news {
  color: #fff;
  /* height: 50rpx;
  line-height: 50rpx; */
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
