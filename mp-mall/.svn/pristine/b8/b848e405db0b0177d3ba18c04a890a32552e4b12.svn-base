}
.bar_header {
  width: 100%;
  position: fixed;
  transition: .3s;
  /* background-color: #fff; */
  z-index: 1;
}
.bar_header.active{
  /* background: linear-gradient(360deg, #fff 0%, #1AE11A 100%); */
  /* background: linear-gradient(360deg, #fff 0%, #36af62 100%); */
  /* background-color: #fff; */
}
.bar_item {
  left: 0;
  width: 100%;
  z-index: 1;
  transition: .3s;
  display: flex;
  align-items: flex-end;
}
.bar_item.active {
  background-color: #fff;
  /* background-color: #1AE11A; */
  /* background: linear-gradient(360deg, #fff 0%, #1AE11A 100%); */
}
.bar_item > text {
  padding: 0 20rpx;
  font-size: 46rpx;
}
.bar_item .search {
  display: flex;
  padding: 0rpx 10rpx;
  font-size: 30rpx;
  min-width: 360rpx;
