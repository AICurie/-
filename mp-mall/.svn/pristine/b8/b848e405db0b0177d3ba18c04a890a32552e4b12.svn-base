}
.news_tips.active .news {
  position: fixed;
  width: 94%;
  z-index: 10;
}
/* .header_top .news .news_item {
  color: #fff;
} */
.news_tips .news .news_item view {
  /* color: #fff; */
  color: #fff;
}
.news_tips .city {
  color: #fff;
}
.header_top .live {
  
  /* padding-right: 20rpx; */
  padding: 20rpx 0;
  display: flex;
  font-size: 32rpx;
  align-items: center;
}
.header_top .live[hidden] {
  display: none;
}
.live .city {
  flex: 1;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.header_top .live image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.header_top .search {
  flex: 1;
  display: flex;
  padding: 10rpx 20rpx;
