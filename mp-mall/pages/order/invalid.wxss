/* pages/orider/invalid.wxss */
.invalid {
  min-height: 100vh;
  background-color: #f2f2f2;
}
.invalid_list {
  overflow: hidden;
  /* background-color: #fff; */
}
.invalid_item {
  border-radius: 10rpx;
  margin: 20rpx 20rpx 0;
  background-color: #fff;
  
}
.invalid_tit {
  padding: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item_tit {
  color: #1D202B;
}
.ff {
  color: #FF983E;
}
.ef {
  color: #EF2322;
}
.item_time {
  padding-top: 10rpx;
  color: #9B9B9B;
}
.goods_nav {
  display: flex;
  padding: 20rpx 30rpx;
  align-items: center;
  justify-content: space-between;
}
.goods_img {
  flex: 1;
  display: flex;
}
.goods_img image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 40rpx;
}
.goods_img image:last-child{
  margin-right: 0;
}
.goods_all {
  display: flex;
  align-items: center;
}
.goods_all .goods_num {
  color: #9B9B9B;
  padding: 0 20rpx;
}
.arrow {
  width: 16rpx;
  height: 16rpx;
  transform: rotateZ(-45deg);
  border-right: 2rpx solid #9B9B9B;
  border-bottom: 2rpx solid #9B9B9B;
}
.tui_price {
  /* color: #333; */
  height: 60rpx;
  margin: 0 20rpx;
  padding: 0 20rpx;
  /* text-align: center; */
  line-height: 60rpx;
  border-radius: 10rpx;
  background-color: #F5F5F5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.tui_price .flex_fl {
  font-size: 30rpx;
  color: #333;
}
.tui_price .flex_fr {
  display: flex;
  align-items: center;
  
}
.tui_price .flex_fr .flex_txt {
  font-size: 24rpx;
  margin-right: 20rpx;
}

.tui_price text {
  color: #333;
  font-size: 20rpx;
}

.invalid_tit {
  display: flex;
  justify-content: space-between;
}
.invalid_tit .goods_fl {
  width: 172rpx;
  height: 172rpx;
}
.invalid_tit .goods_fl image {
  width: 100%;
  height: 100%;
}
.invalid_tit .goods_fr {
  flex: 1;
  display: flex;
  height: 172rpx;
  margin-left: 20rpx;
  flex-direction: column;
  justify-content: space-between;
}
.invalid_tit .goods_fr .goods_name {
  display: flex;
  justify-content: space-between;
}
.invalid_tit .goods_fr .goods_name .name_fl {
  flex: 1;
  display: flex;
}
.invalid_tit .goods_fr .goods_name .name_fr {
  color: #F15A24;
}
.invalid_tit .goods_fr .goods_name .name_fl .tab {
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  text-align: center;
  border-radius: 10rpx;
  /* background-color: #c69c6d; */
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.invalid_tit .goods_fr .goods_name .name_fl .tab.active {
  color: #fefb58;
  background: linear-gradient(to right, #f54327 0%, #f54327 100%);
}
.invalid_tit .goods_fr .goods_name .name_fl .tab image {
  width: 100%;
  height: 100%;
}
.invalid_tit .goods_fr .goods_name .name_fl .text {
  font-size: 30rpx;
  font-weight: 700;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.invalid_tit .goods_fr .goods_name .name_fl .text.tab_text {
  text-indent: 140rpx;
}
.invalid_tit .goods_fr .goods_price .price_txt {
  font-size: 20rpx;
  color: #999;
}
.invalid_tit .goods_fr .goods_price .price_box {
  display: flex;
  align-items: flex-end;
}
.invalid_tit .goods_fr .goods_price .price_box .price {
  display: flex;
  align-items: flex-end;
}
.invalid_tit .goods_fr .goods_price .price_box .price .txt {
  font-size: 20rpx;
  color: #999;
}
.invalid_tit .goods_fr .goods_price .price_box .price .util {
  font-size: 20rpx;
  color: #FF0000;
}
.invalid_tit .goods_fr .goods_price .price_box .price .num {
  font-size: 32rpx;
  color: #FF0000;
  font-weight: 700;
  height: 40rpx;
}
.invalid_tit .goods_fr .goods_price .price_box .goods_num {
  font-size: 22rpx;
  color: #999;
  margin-left: 20rpx;
}
.fn_list {
  display: flex;
  padding: 20rpx;
  justify-content: flex-end;
}
.fn_item {
  /* margin-left: 16rpx; */
  height: 46rpx;
  line-height: 46rpx;
  padding: 0 12rpx;
  border-radius: 30rpx;
  border: 1rpx solid #797979;
}

.fn_item.active {
  font-weight: 700;
  color: #fff;
  border: none;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 16rpx;
  /* background-color: #1AE11A; */
  /* border: 1rpx solid #EF2322; */
/* background: linear-gradient(to right, #5ad563 0%, #36af62 100%); */
background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.recom {
  margin: 20rpx;
}
.recom .recom_tit {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
}
.recom .recom_tit .recom_tit_bx {
  display: flex;
  align-items: center;
  justify-content: center;
}
.recom .recom_tit .recom_tit_bx .div {
  width: 30rpx;
  height: 4rpx;
  margin: 0 10rpx;
  background-color: #333;
}
.recom .recom_tit .recom_tit_bx .tit {
  font-weight: 700;
}
.recom .recom_box {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.recom .recom_box .recom_item {
  width: 49%;
  margin-top: 10rpx;
  margin-right: 2%;
  overflow: hidden;
  border-radius: 10rpx;
}
.recom .recom_box .recom_item:nth-child(2n) {
  margin-right: 0;
}
.recom .recom_box .recom_item .item_img {
  width: 100%;
  height: 318rpx;
}
.recom .recom_box .recom_item .item_img image {
  width: 100%;
  height: 100%;
}
.recom .recom_box .recom_item .item_info {
  padding: 20rpx;
  background-color: #fff;
}
.recom .recom_box .recom_item .item_info .info_tit {
  display: flex;
  margin: 10rpx 0;
  align-items: center;
}
.recom .recom_box .recom_item .item_info .info_tit .tab {
  display: inline-block;
  width: 140rpx;
  height: 40rpx;
  color: #fff;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  position: absolute;
  text-align: center;
  border-radius: 10rpx;
  /* background-color: #c69c6d; */
  /* background: linear-gradient(to right, #5ad563 0%, #36af62 100%); */
  background: linear-gradient(to right, #60b533  0%, #60b732 100%);
}
.recom .recom_box .recom_item .item_info .info_tit .text {
  flex: 1;
  font-size: 30rpx;
  overflow: hidden;
  display: inline-block;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.recom .recom_box .recom_item .item_info .info_tit .text.tab_txt {
  text-indent: 150rpx;
}
.recom .recom_box .recom_item .item_info .add_car {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  justify-content: space-between;
}
.recom .recom_box .recom_item .item_info .in_fl {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price {
  display: flex;
  align-items: flex-end;
}
.recom .recom_box .recom_item .item_info .in_fl .price .util {
  font-size: 20rpx;
  color: #FF0000;
}
.recom .recom_box .recom_item .item_info .in_fl .price .num {
  font-size: 34rpx;
  height: 40rpx;
  color: #FF0000;
  font-weight: 700;

}
.recom .recom_box .recom_item .item_info .in_fl .price .price_txt {
  font-size: 20rpx;
}
.recom .recom_box .recom_item .item_info .in_fl .sales {
  color: #999;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr {
  width: 40rpx;
  height: 40rpx;
}
.recom .recom_box .recom_item .item_info .add_car .in_fr image {
  width: 100%;
  height: 100%;
}



